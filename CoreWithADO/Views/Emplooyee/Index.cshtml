@model IEnumerable<CoreWithADO.Models.Employees>
@{
}
<a asp-action="Create" asp-controller="Emplooyee"  class="btn btn-info">Create New Employee</a>
<table class="table table-bordered">
    <thead>
        <tr>
            <th>Id</th>
            <th>Fname</th>
            <th>Lname</th>
            <th>Email</th>
            <th>Mobile</th>
            <th>Salary</th>
            <th>Gender</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var em in Model)
            {
                <tr>
                    <td>@em.id</td>
                    <td>@em.fname</td>
                    <td>@em.lname</td>
                    <td>@em.email</td>
                    <td>@em.mobile</td>
                    <td>@em.salary</td>
                    <td>@em.gender</td>
                    <td>
                        <a asp-action="Update" asp-controller="Emplooyee" asp-route-id="@em.id" class="btn btn-info">Edit</a>||
                        <a asp-action="Delete" asp-controller="Emplooyee" asp-route-id="@em.id" class="btn btn-info">Delete</a>

                    </td>
                </tr>
            }
    </tbody>
</table>